{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "",
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    },
    "jsx": "react-native",
    "types": ["react", "react-native"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/colorList.tsx",
    "app/(main)/(tabs)/tabBar.jsx",
    "src/**/*"
  ]
}
